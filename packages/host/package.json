{
  "name": "@wetrial/host",
  "version": "0.0.1",
  "private": true,
  "description": "wetrial模块化子应用的宿主应用,用于开发子应用",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "umi build",
    "pub": "node scripts/publish",
    "patch": "yarn version --patch && git push origin master && git push origin --tags",
    "minor": "yarn version --minor && git push origin master && git push origin --tags",
    "major": "yarn version --major && git push origin master && git push origin --tags",
    "new": "node scripts/template",
    "docker:build": "docker-compose -f ./docker/docker-compose.yml build",
    "docker:push": "npm run docker-hub:build && npm run docker:tag && docker push wetrial/template",
    "docker:tag": "docker tag template wetrial/template",
    "fetch:blocks": "pro fetch-blocks --branch antd@4 && npm run prettier",
    "functions:build": "netlify-lambda build ./lambda",
    "functions:run": "cross-env NODE_ENV=dev netlify-lambda serve ./lambda",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "start": "cross-env REACT_APP_ENV=dev umi dev",
    "dev": "npm run start:dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "test": "umi test",
    "test:all": "node ./tests/run-tests.js",
    "test:component": "umi test ./src/components",
    "tsc": "tsc",
    "ui": "umi ui"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.8",
    "@wetrial/component": "^0.0.11",
    "@wetrial/core": "^2.0.0",
    "@wetrial/hooks": "^0.11.1",
    "@wetrial/template": "^0.0.1",
    "antd": "^4.0.4",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.22",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "path-to-regexp": "^6.1.0",
    "qs": "^6.9.1",
    "react": "^16.13.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "redux": "^4.0.5",
    "use-merge-value": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@umijs/fabric": "^2.0.6",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/test": "^3.0.14",
    "antd-dayjs-webpack-plugin": "^0.0.9",
    "chalk": "^3.0.0",
    "cross-env": "^7.0.0",
    "cross-port-killer": "^1.2.1",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "husky": "^4.2.3",
    "jest-puppeteer": "^4.4.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.0.7",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.19.1",
    "serverless-http": "^2.3.1",
    "stylelint": "^13.2.0",
    "typescript": "^3.8.3",
    "umi": "^3.0.14",
    "yorkie": "^2.0.0"
  },
  "optionalDependencies": {
    "puppeteer": "^2.1.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ],
  "license": "MIT"
}
